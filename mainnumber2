#include <iostream>
#include "stackLi.h"
#include "QUEUE_LIST.h"

QUEUE_LIST<int> Q;
int c=1;
int b=1;

using namespace std;

int main()
{
	do
	{
		cout << "Enter the coefficient of a term(0 0) to stop: ";
		cin >> c;
		cin >> b;
		Q.pushq(c, b);
		
	} while (c != 0 || b !=0);
	
	int base;
	cout << "enter a base";
	cin >> base;
	int r=0;
	int total=0 ;
	cout << "F" << "(" << base << ")=";
	while (Q.Emptyq() != true )		//|| S.Emptys()!=NULL //print stack
	{
		Q.popq(c,b);
		
		r = Q.calculations(c, b, base);
		total += r;
		
	}
	
	
	
	cout << total;
	


	system("pause");
	return 0;
}
